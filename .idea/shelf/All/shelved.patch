+            List<Broker__c> brokers = (List<Broker__c>) JSON.deserialize(
+                brokersJSON,
+                List<Broker__c>.class
+            );
+            insert brokers;
+
+            StaticResource propertiesResource = [
+                SELECT Id, Body

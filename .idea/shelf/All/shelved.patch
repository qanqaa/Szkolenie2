+
+        if (center && center.lat && center.long) {
+            component.map.setView(center);
+        }
+
+        if (component.layerGroup) {
+            component.map.removeLayer(component.layerGroup);
+        }
+
+        var properties = component.get('v.properties');
+        if (!properties) {
+            return nodes;

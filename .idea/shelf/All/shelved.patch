+<apex:page id="HOSP_EditDoctor" standardController="Doctor__c" extensions="HOSP_EditDoctorController"
+           tabStyle="Doctor__c">
+    <apex:form>
+        <apex:sectionHeader title="{!$Label.Doctor}" subtitle="{!Doctor__c.Name}"/>
